{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown", "jsdoc-plugin-typescript"],
	"typescript": {
		"moduleRoot": "src"
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": true
	},
	"opts": {
		"destination": "docs",
		"recurse": true,
		"readme": "README.md"
	}
}
